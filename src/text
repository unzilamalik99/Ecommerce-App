"user client"
import axios from "axios";
import { useEffect, useState } from "react";

const App=()=> {
 const [users, setusers] = useState([])
  const getData=async ()=>{
    const {data}= await axios.get("https://jsonplaceholder.typicode.com/users")
    setusers(data)
  }
useEffect(() => {
 getData()

}, [])

  return (
 <>
<div className='p-20'>
<button  onClick={getData}className='bg-black text-white px-5 py-3 rounded-2xl '>Get data </button>
<div className="bg-slate-100 mt-5 p-8">
  
    <ul>{
      users.map((e)=>{
        return <li>
       {e.username}----<a id="page" href={`/page${e.id}`}>explore</a>
        </li>


      })}
    </ul>
  
</div>
</div>
 </>
  );
}

export default App;
